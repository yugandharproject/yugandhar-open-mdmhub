<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="DECIMAL" hibernate-type="integer"
			precision="22" not-null="true">
		</sql-type>
		<sql-type jdbc-type="DECIMAL" hibernate-type="integer"
			precision="22" not-null="false">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"
			length="26" not-null="true">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"
			length="26" not-null="false">
		</sql-type>
	</type-mapping>
	<table-filter match-name="facilitydemo" match-catalog="yug_owner"></table-filter>
</hibernate-reverse-engineering>